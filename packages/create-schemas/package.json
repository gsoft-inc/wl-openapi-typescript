{
    "name": "@workleap/create-schemas",
    "author": "Workleap",
    "version": "0.0.1",
    "description": "CLI to generate Typescript schemas from an OpenAPI document",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-openapi-typescript.git",
        "directory": "packages/create-schemas"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
    "bin": "./dist/bin.js",
    "scripts": {
        "dev": "pnpm run build && pnpm run dev:link && pnpm run dev:tsup",
        "dev:link": "pnpm link --global",
        "dev:tsup": "tsup --config tsup.dev.ts",
        "build": "tsup --config tsup.build.ts"
    },
    "devDependencies": {
        "@swc/core": "1.3.68",
        "@swc/helpers": "0.5.1",
        "@swc/jest": "0.2.26",
        "@types/jest": "29.5.3",
        "@types/node": "18.16.19",
        "@typescript-eslint/parser": "5.62.0",
        "@workleap/eslint-plugin": "1.8.2",
        "@workleap/swc-configs": "1.0.0",
        "@workleap/tsup-configs": "2.0.0",
        "eslint": "8.56.0",
        "tsup": "7.1.0",
        "typescript": "5.0.4"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
